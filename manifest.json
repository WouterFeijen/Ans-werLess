{

  "description": "Remove answers from results in Ans-App quizzes, so that practice, without seeing the answers, is possible again.",
  "manifest_version": 2,
  "name": "Ans-werLess",
  "version": "1.0",
  "homepage_url": "https://github.com/WouterFeijen/Ans-werLess",
  "icons": {
    "48": "icons/-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/answerLess-icon32.png",
    "theme_icons": [
      {
        "light": "icons/light/answerLess-icon32-light.png",
        "dark": "icons/dark/answerLess-icon32-dark.png",
        "size": 32
      },

      {
        "light": "icons/light/answerLess-icon48-light.png",
        "dark": "icons/dark/answerLess-icon48-dark.png",
        "size": 48
      },
      
      {
        "light": "icons/light/answerLess-icon128-light.png",
        "dark": "icons/dark/answerLess-icon128-dark.png",
        "size": 128
      }],
    "default_title": "Ans-werLess",
    "default_popup": "popup/menu.html"
  }

}
